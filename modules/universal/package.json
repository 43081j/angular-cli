    "angular2-express-engine": "file:../express-engine",
    "angular2-hapi-engine": "file:../hapi-engine",
    "angular2": "2.0.0-beta.11",
    "preboot": "file:../preboot",
    "zone.js": "~0.6.4",
    "es6-shim": "^0.35.0",
    "es6-promise": "^3.0.2",
    "reflect-metadata": "0.1.2",
    "typescript": "~1.8.9",
    "rxjs": "~5.0.0-beta.2",
    "css": "^2.2.1",
    "parse5": "^1.5.0"
  },
  "dependencies": {
    "angular2-universal-polyfills": "~0.1.3",
    "angular2-express-engine": "*",
    "angular2-hapi-engine": "*",
    "xhr2": "^0.1.3",
    "css": "^2.2.1",
    "parse5": "^1.5.0"
  },
  "peerDependencies": {
    "preboot": ">=2.0.4",
    "angular2-express-engine": ">=0.6.1",
