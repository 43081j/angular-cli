    "build": "tsc || true",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/universal"
  },
  "bugs": {
    "url": "https://github.com/angular/universal/issues"
  },
  "devDependencies": {
    "angular2": "2.0.0-beta.7",
    "angular2-universal-preview": "file:../universal",
    "rimraf": "^2.5.1",
    "typescript": "^1.7.3"
  },
  "dependencies": {
  },
  "peerDependencies": {
    "angular2-universal-preview": "*"
  }
}
